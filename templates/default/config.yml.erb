settings:
  info_server: <%= @etcd_server %>
  port: <%= @node['whatsinstalled']['port'] %>
  check_interval: <%= node['whatsinstalled']['check_interval'] %>
  ttl: <%= node['whatsinstalled']['ttl'] %>
<% unless @apps.nil? %>
apps:
  <% @apps.each do |app, path| -%>
  <%= app %>: <%= path %>
  <% end %>
<% end %>
<% if @assays %>
assays: <%= @assays %>
<% end %>
<% unless @packages.nil? %>
packages:
<% @packages.each do |package| -%>
  - <%= package %>
<% end %>
<% end %>
